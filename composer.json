{
    "name": "brainsrage/infoflot-laravel",
    "description": "Laravel Package for Infoflot PHP Client",
    "keywords": [
        "php",
        "infoflot",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "CryptoWeb\\Infoflot\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Xmk",
            "email": ""
        }
    ],
    "require": {
        "cryptoweb/infoflot-api": "*",
        "laravel/framework": "^9.0|^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CryptoWeb\\Infoflot\\InfoflotServiceProvider"
            ],
            "aliases": {
                "Infoflot": "CryptoWeb\\Infoflot\\Facades\\Infoflot"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
